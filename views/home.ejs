<%- include('header')%>
<h2>Open roles:</h2>
<% categorias.forEach(categoria => { %>
    
    <div>
        <h3><%- categoria.categoria %></h3>
        <ul>
            <% categoria.vagas.forEach(vagas => { %>
                <li><a href="vaga/<%- vagas.id %>"> <%- vagas.titulo %> </a></li>
            <% } ) %>
        </ul>
    </div>
    
<% } ) %>
<%- include('footer') %>