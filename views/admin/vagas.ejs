<%- include('header')%>
<h2 class="gray">Admin Vaga:
    <a href="/admin/vagas/nova">Nova</a> |
    <a href="/admin/">Voltar</a>
</h2>
    
    <div class="lista"> 
        <ul class="list-group">
            <% vagas.forEach( vaga => { %>
                <li class="list-group-item list-group-item-action flex-column align-items-start">
                    <%- vaga.titulo %> |
                    <a href="/admin/vagas/delete/<%- vaga.id %>">
                        Excluir
                    </a> |
                    <a href="/admin/vagas/editar/<%- vaga.id %>">
                        Editar
                    </a>
                </li>
            <% } ) %>
        </ul>
    </div>   

<%- include('footer') %>